{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "singleAttributePerLine": true,

    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "<BUILTIN_MODULES>",
        "",
        "<TYPES>",
        "",
        "<THIRD_PARTY_MODULES>",
        "",
        "<TYPES>^[.]",
        "",
        "^@(/.*)$",
        "^[.]"
    ],
    "importOrderTypeScriptVersion": "5.2.2",
    "importOrderParserPlugins": ["typescript"]
}
