type Task {
    id: ID!
    title: String!,
    description: String!,
    dueDate: String
    creator: Person
    assigneeId: ID
    assignee: Person
}

type Person {
    id: ID!
    firstname: String
    lastname: String,
    email: String!
    tasks: [TaskOfPerson]
}

type TaskOfPerson {
    id: ID!
    title: String!,
    description: String!,
    dueDate: String
    creatorId: ID
    assigneeId: ID
}

type Query {
    getTasks: [Task]!
    getPersons: [Person]!
}